{
  "name": "AdminWebsite",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod --aot",
    "test": "ng test --code-coverage",
    "test-once": "ng test --watch=false --code-coverage --source-map=true",
    "test-once-ci": "ng test --browsers=ChromeHeadless --watch=false --code-coverage --source-map=true",
    "lint": "ng lint && npm run prettify",
    "prettify": "run-script-os",
    "prettify:nix": "npx prettier -c 'src/app/**/*.*'",
    "prettify:default": "npx prettier -c \"src\\app\\**\\*.*\"",
    "prettify-fix": "run-script-os",
    "prettify-fix:nix": "npx prettier --write 'src/app/**/*.*'",
    "prettify-fix:default": "npx prettier --write \"src\\app\\**\\*.*\"",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "update-api-client": "nswag run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.2.0",
    "@angular/common": "^10.2.0",
    "@angular/compiler": "^10.2.0",
